{
  "name": "@kizzlebot/cf-tcp-mock",
  "description": "cloudflare worker polyfill for net, tls, and dns for making mongodb connections work in workerd",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./net": {
      "import": "./dist/net.js",
      "types": "./dist/net.d.ts"
    },
    "./tls": {
      "import": "./dist/tls.js",
      "types": "./dist/tls.d.ts"
    },
    "./dns": {
      "import": "./dist/dns.js",
      "types": "./dist/dns.d.ts"
    }
  },
  "./net-mock": {
    "import": "./dist/net-mock.js",
    "types": "./dist/net-mock.d.ts"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "keywords": [
    "cloudflare",
    "worker",
    "mongodb",
    "polyfill"
  ],
  "author": "James Choi <choijjames@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241205.0",
    "@types/node": "^22.10.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}